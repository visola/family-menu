{
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "arrow-body-style": "off",
    "arrow-parens": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "no-use-before-define": "off"
  }
}
